            "User": {
                "title": "User",
                "properties": {
                    "id": {
                        "type": "integer",
                        "example": 1
                    },
                    "name": {
                        "type": "string",
                        "example": "Zikrullo"
                    },
                    "email": {
                        "type": "string",
                        "example": "zikrullo@gmail.com"
                    },
                    "email_verified_at": {
                        "type": "string",
                        "format": "date-time",
                        "example": "2025-11-06T10:00:00Z",
                        "nullable": true
                    },
                    "password": {
                        "type": "string",
                        "example": "$2y$10$hashedpassword"
                    },
                    "created_at": {
                        "type": "string",
                        "format": "date-time",
                        "example": "2025-11-06T10:00:00Z"
                    },
                    "updated_at": {
                        "type": "string",
                        "format": "date-time",
                        "example": "2025-11-06T10:00:00Z"
                    }
                },
                "type": "object"
            }
        }
    },
    "tags": [
